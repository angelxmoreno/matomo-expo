{
  "name": "matomo-tracker-react-native-v2",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "docker:start": "docker-compose -f docker/docker-compose.yml up --remove-orphans",
    "docker:start:detached": "docker-compose -f docker/docker-compose.yml up -d --remove-orphans",
    "docker:stop": "docker-compose -f docker/docker-compose.yml stop",
    "docker:rm": "docker-compose -f docker/docker-compose.yml rm",
    "test": "jest",
    "test:integration": "mocha tests/integration/**/*.spec.ts",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "expo-constants": "~14.4.2",
    "expo-crypto": "^12.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.2",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.6",
    "axios-mock-adapter": "^1.22.0",
    "expo": "~49.0.15",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "jest-expo",
    "roots": [
      "tests"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
