{
  "name": "matomo-tracker-react-native-v2",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "docker:start": "docker-compose -f docker/docker-compose.yml up --remove-orphans",
    "docker:start:detached": "docker-compose -f docker/docker-compose.yml up -d --remove-orphans",
    "docker:stop": "docker-compose -f docker/docker-compose.yml stop",
    "docker:rm": "docker-compose -f docker/docker-compose.yml rm",
    "test": "mocha",
    "build": "rimraf dist && tsc"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.2",
    "@types/react-native": "^0.72.6",
    "axios-mock-adapter": "^1.22.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "react-native-mock-render": "^0.1.9",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "expo-constants": "^14.4.2",
    "expo-crypto": "^12.6.0",
    "react": "^18.2.0",
    "react-native": "^0.72.7"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "tests/**/*.spec.ts",
    "require": [
      "ts-node/register"
    ],
    "recursive": true
  }
}
